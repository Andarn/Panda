group example.test;

import panda.lang;

class Foo extends Test {

}

class Test {

    constructor(String message) {
        System.print(message);
    }

    method echo(String message) {
        System.print(message);
    }

}

main {
    Test test = new Test("Constructor");
    test.echo("Method");

    Foo varFoo = null;
    varFoo = new Foo();

    String s = null;
    s = "Hello Panda x2";

    System.print("Hello Panda", "Test", "X", true, false, varFoo, s, test);

    if (false) {
        System.print("Nope");
    }
    else {
        System.print("Yay");
    }

    loop (2) {
        System.print("Loop x1");

        loop (2) {
            System.print("Loop x2");
            break;
        }

        continue;
        System.print("Loop x3");
    }

    while (true) {
        System.print("While x1");
        break;
    }
}